defc	commands=4
defc	stacksig=$f731

defvars	worksp
{
        dbg_buffer	ds.b 8  ; First value for debugging purposes only
        lastclv	        ds.b 2
        seqptr	        ds.l 1
        inhandle	ds.w 1
        outhandle	ds.w 1
        oihandle	ds.w 1
        lastseg	        ds.w 1
        onepercent	ds.b 3
        header	        ds.b 30
        openfiles	ds.b 1
        options	        ds.b 1
        lastmsg	        ds.w 1
        redrawflag	ds.b 1
        curpos	        ds.b 2
        workarea	ds.b 1770
        listptr	        ds.w 2
        progpercent	ds.b 1
        cursizeK	ds.b 3
        bigfile	        ds.b 1
        flushsize	ds.w 1
        progmessage	ds.b 16
}
