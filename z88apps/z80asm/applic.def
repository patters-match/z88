; Overview of bank usage for 48K Application:
;
; Bank $3D (61):    Token base, z80asm MTH.
; Bank $3E (62):    z80asm application bank 0 (addressed for $8000 onwards)
; Bank $3F (63):    z80asm application bank 1 (addressed for $C000 onwards)
; Bank $3F (63):    ROM Header ($3FC0)


DEFGROUP  {
               z80asm_MTH_bank = $3D,
               tokens_bank = z80asm_MTH_bank,
               Z80asm_bank1,
               z80asm_bank2
          }

DEFC Token_base = 0
DEFC MTH_Z80asm_ORG = $400

DEFC Z80asm_DOR = $8000, Z80asm_workspace = $1ffe-$1b00
