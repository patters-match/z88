; Installer Application Definitions

        defc    appl_bank=$3f
        defc    safe=512
        defc    unsafe=0
        defc    scratch=$1ffe-safe
        defc    maxcommand=6


defvars scratch {
                singlego        ds.b    1
                menucommand     ds.b    1
                filebuffer      ds.b    headerlength
                extension       ds.w    1
                workparams      ds.b    13
                pad             ds.b    padlength
                explicitname    ds.b    255
                }

